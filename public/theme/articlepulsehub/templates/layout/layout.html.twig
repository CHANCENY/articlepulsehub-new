<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
    <link rel="manifest" href="/site.webmanifest">
    <link href="https://fonts.googleapis.com/css?family=Roboto:100,100i,300,300i,400,400i,500,500i,700,700i,900,900i&display=swap" rel="stylesheet">
    <!-- Include the library CSS and JS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/cookieconsent@3/build/cookieconsent.min.css" />
    <script src="https://cdn.jsdelivr.net/npm/cookieconsent@3/build/cookieconsent.min.js"></script>

    <link href="/theme/articlepulsehub/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" href="/theme/articlepulsehub/assets/css/fontawesome.css">
    <link rel="stylesheet" href="/theme/articlepulsehub/assets/css/templatemo-stand-blog.css">
    <link rel="stylesheet" href="/theme/articlepulsehub/assets/css/owl.css">

    {% if theme.assets.head is defined %}
        {% for file_asset in theme.assets.head %}

            {{ file_asset|raw }}

        {% endfor %}
    {% endif %}

    {% block head %}{% endblock %}
    <!--In banner ads-->
    <script async="" src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js?client=ca-pub-7521938997713716" crossorigin="anonymous"></script>&nbsp;<script>
        (adsbygoogle = window.adsbygoogle || []).push({});
    </script>
    <title>{% block title %} {{ page_title }}  {% endblock %}</title>
    <!-- Google tag (gtag.js) -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-VBYEHHT4SL"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag(){dataLayer.push(arguments);}
        gtag('js', new Date());

        gtag('config', 'G-VBYEHHT4SL');
    </script>
</head>
<body class="{{ body_class }}">

<div id="preloader">
    <div class="jumper">
        <div></div>
        <div></div>
        <div></div>
    </div>
</div>
<header class="">
    <nav class="navbar navbar-expand-lg">
        <div class="container">
            <a class="navbar-brand" href="/"><h2>{{ site.get('site_name') }}<em>.</em></h2></a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarResponsive">
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item {{ 'university.home.landing'|active_menu }}">
                        <a class="nav-link" href="/">Home
                            <span class="sr-only">(current)</span>
                        </a>
                    </li>
                    <li class="nav-item {{ 'services.about.us'|active_menu }}">
                        <a class="nav-link" href="{{ url('services.about.us') }}">About Us</a>
                    </li>
                    <li class="nav-item {{ 'services.articles'|active_menu }}">
                        <a class="nav-link" href="{{ url('services.articles') }}">Articles</a>
                    </li>
                    <li class="nav-item {{ 'services.contact.us'|active_menu }}">
                        <a class="nav-link" href="{{ url('services.contact.us') }}">Contact Us</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
</header>

{% block banner %} {% endblock %}

{% block featured %} {% endblock %}

{% block call_action %} {% endblock %}

<main>
    {% block body %}{% endblock %}
</main>


<footer>
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <ul class="social-icons">
                    <li><a href="https://www.facebook.com/freeman.nyasulu.5">Facebook</a></li>
                    <li><a href="https://www.instagram.com/chancenyasulu?igsh=cmN1Znl1bjk5eTQz">Instagram</a></li>
                    <li><a href="{{ url('services.policy') }}">Terms, Condition and privacy policies</a></li>
                </ul>
            </div>
            <div class="col-lg-12">
                <div class="copyright-text">
                    <p>Copyright 2020 {{ site.get('site_name') }}.

                        | Design: <a rel="nofollow" href="https://templatemo.com" target="_parent">TemplateMo</a></p>
                </div>
            </div>
        </div>
    </div>
</footer>
<script src="/theme/articlepulsehub/vendor/jquery/jquery.min.js"></script>
<script src="/theme/articlepulsehub/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
<script src="/theme/articlepulsehub/assets/js/custom.js"></script>
<script src="/theme/articlepulsehub/assets/js/owl.js"></script>
<script src="/theme/articlepulsehub/assets/js/slick.js"></script>
<script src="/theme/articlepulsehub/assets/js/isotope.js"></script>
<script src="/theme/articlepulsehub/assets/js/accordions.js"></script>
{% if theme.assets.footer is defined %}
    {% for file_assetf in theme.assets.footer %}

        {{ file_assetf|raw }}

    {% endfor %}
{% endif %}
<script language = "text/Javascript">
    cleared[0] = cleared[1] = cleared[2] = 0; //set a cleared flag for each field
    function clearField(t){                   //declaring the array outside of the
        if(! cleared[t.id]){                      // function makes it static and global
            cleared[t.id] = 1;  // you could use true and false, but that's more typing
            t.value='';         // with more chance of typos
            t.style.color='#fff';
        }
    }
</script>
<script>
    window.addEventListener("load", function(){
        window.cookieconsent.initialise({
            palette: {
                popup: { background: "#333", text: "#fff" },
                button: { background: "#4CAF50", text: "#fff" }
            },
            theme: "classic",
            position: "bottom-right",
            content: {
                message: "This website uses cookies to ensure you get the best experience.",
                dismiss: "Accept",
                link: "Learn more",
                href: "/en/privacy-policy"
            }
        });
    });
</script>
<script>
    if ('serviceWorker' in navigator) {
        window.addEventListener('load', () => {
            navigator.serviceWorker.register('/service-worker.js')
                .then(reg => console.log('Service Worker registered:', reg.scope))
                .catch(err => console.log('Service Worker registration failed:', err));
        });
    }
</script>

</body>
</html>